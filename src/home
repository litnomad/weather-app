// home content as default
const content = document.querySelector('#content');
content.innerHTML = `
    <div>
      <h1>Welcome to Our Restaurant</h1>
      <p>Enjoy the finest dishes crafted with love and passion.</p>
    </div>
  `;

function renderHome() {
    const content = document.querySelector('#content');
    content.innerHTML = `
    <div>
      <h1>Welcome to Our Restaurant</h1>
      <p>Enjoy the finest dishes crafted with love and passion.</p>
    </div>
  `;
}

export { renderHome };